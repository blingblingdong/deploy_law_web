[{"type":"custom_card","card_type":"law","data":{"chapter":"民法","lines":[{"attributes":{"class":"law-block-line","height":null,"id":null,"src":null,"style":null,"width":null},"children":[{"attributes":null,"text":"以建築物設定抵押權者，於法院拍賣抵押物時，其抵押物存在","type":"text"},{"attributes":{"class":null,"height":null,"id":null,"src":null,"style":"color:hsl(30,75%,60%);","width":null},"children":[{"attributes":null,"text":"所必要之權利得讓與者","type":"text"}],"type":"span"},{"attributes":null,"text":"，應併付拍賣。但抵押權人對於該權利賣得之價金，無優先受清償之權。","type":"text"}],"line_type":"normal"},{"attributes":{"class":"law-indent","height":null,"id":null,"src":null,"style":null,"width":null},"children":[{"attributes":null,"text":"我愛你","type":"text"}],"line_type":"indent"}],"num":"877-1"}}]